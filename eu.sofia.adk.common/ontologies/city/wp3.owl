  <!--                                                                      -->
  <!--                               CLASSES                                -->
  <!--                                                                      -->
  
  <!-- COMMANDS -->

  <!-- Command -->
  <owl:Class rdf:about="#Command">
    <rdfs:comment xml:lang="en">Command supported by an actuator.</rdfs:comment>
    <rdfs:comment xml:lang="en">The set of instructions needed to be intechange between smart devices in order to control the basic directives of gathering information.</rdfs:comment>
    <rdfs:comment xml:lang="en">A command generated by a SOFIA application on the client side and addressed to a SOFIA system/device/server.</rdfs:comment>
  </owl:Class>
  
  <!-- Activation Command -->
  <owl:Class rdf:about="#ActivationCommand">
    <rdfs:comment xml:lang="en">The command set to activate a device or set of devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Command"/>
  </owl:Class>
  
  <!-- CCTV Activation Command -->
  <owl:Class rdf:ID="CCTVActivationCommand">
    <rdfs:subClassOf rdf:resource="#ActivationCommand"/>
  </owl:Class>
  
  <!-- Motion Detection Command -->
  <owl:Class rdf:ID="MotionDetectionCommand">
    <rdfs:subClassOf rdf:resource="#CCTVActivationCommand"/>
  </owl:Class>
  
  <!-- Object Recognition Command -->
  <owl:Class rdf:ID="ObjectRecognitionCommand">
    <rdfs:comment xml:lang="en">Commands associated to object recognition where refers to the capability to detect a specific object in the video streaming</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#CCTVActivationCommand"/>
  </owl:Class>
  
  <!-- Wireless Sensor Network Activation Command -->
  <owl:Class rdf:ID="WSNActivationCommand">
    <rdfs:subClassOf rdf:resource="#ActivationCommand"/>
  </owl:Class>
  
  <!-- Presence Detection Command -->
  <owl:Class rdf:ID="PresenceDetectionCommand">
    <rdfs:subClassOf rdf:resource="#WSNActivationCommand"/>
  </owl:Class>
  
  <!-- Configuration Command -->
  <owl:Class rdf:ID="ConfigurationCommand">
    <rdfs:comment xml:lang="en">The set of directives directed to configure the smart devices.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Command"/>
  </owl:Class>

  <!-- CCTV Configuration Command -->
  <owl:Class rdf:about="#CCTVConfigurationCommand">
    <rdfs:subClassOf rdf:resource="#ConfigurationCommand"/>
  </owl:Class>
  
  <!-- Motion Detection Configuration Command -->
  <owl:Class rdf:ID="MotionDetectionConfigurationCommand">
    <rdfs:subClassOf rdf:resource="#CCTVConfigurationCommand"/>
  </owl:Class>
  
  <!-- Object Recognition Configuration Command -->
  <owl:Class rdf:ID="ObjectRecognitionConfigurationCommand">
