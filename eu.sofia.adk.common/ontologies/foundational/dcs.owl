    <rdfs:domain rdf:resource="#ImageOutputCapability"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="inputFormat">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#AcousticInputModalityCapability"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="supportedCodec">
    <rdfs:domain rdf:resource="#AcousticModalityCapability"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="pixelAspectRatio">
    <rdfs:domain rdf:resource="#VisualModalityCapability"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="networkProtocol">
    <rdfs:domain rdf:resource="#NetworkCapability"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
