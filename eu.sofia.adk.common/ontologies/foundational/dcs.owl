    <rdfs:domain rdf:resource="#TouchpadInputCapability"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="supportedCharset">
