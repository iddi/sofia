    <rdfs:range rdf:resource="http://ontology.ist-spice.org/mobile-ontology/1/0/core/0/core.owl#Device"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="maxSoundLevel">
