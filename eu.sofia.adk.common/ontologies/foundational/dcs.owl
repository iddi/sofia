    <rdfs:comment xml:lang="en">The dcs:supportsModality property indicates that a modality service accepts or provides a specific modality.
Responsible: UniS</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="videoCodec">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#VideoOutputCapability"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="imageCodec">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
