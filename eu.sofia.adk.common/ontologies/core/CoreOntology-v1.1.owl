<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY DOLCE-Lite "http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#" >
    <!ENTITY CoreOntology-v1 "http://emb1.esilab.org/sofia/ontology/1.3/core/CoreOntology-v1.1.owl#" >
    <!ENTITY SpatialRelations "http://www.loa-cnr.it/ontologies/SpatialRelations.owl#" >
    <!ENTITY TemporalRelations "http://www.loa-cnr.it/ontologies/TemporalRelations.owl#" >
]>


<rdf:RDF xmlns="http://emb1.esilab.org/sofia/ontology/1.3/core/CoreOntology-v1.1.owl#"
     xml:base="http://emb1.esilab.org/sofia/ontology/1.3/core/CoreOntology-v1.1.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:DOLCE-Lite="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:CoreOntology-v1="http://emb1.esilab.org/sofia/ontology/1.3/core/CoreOntology-v1.1.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:SpatialRelations="http://www.loa-cnr.it/ontologies/SpatialRelations.owl#"
     xmlns:TemporalRelations="http://www.loa-cnr.it/ontologies/TemporalRelations.owl#">
    <owl:Ontology rdf:about="">
        <rdfs:label
            >(en) SOFIA Core Ontology v1.1</rdfs:label>
        <owl:versionInfo
            >SOFIA Core Ontology v1.1</owl:versionInfo>
        <rdfs:comment xml:lang="en">Created by Alessandra Toninelli based on the SOFIA Core Ontology v1.0 provided by ESI. Properties defined in DOLCE Lite were removed from CoreOntology.owl and imported from DOLCE-Lite.owl for improved interoperability. Imported additional modules that integrate with DOLCE Lite to support more advanced modeling: 1) Spatial Relations 2) Temporal Relations.
Some proposed changes to the Core Ontology v1.0 are tracked in a separate rtf file</rdfs:comment>
        <rdfs:comment xml:lang="en">Updated with the repository new URL</rdfs:comment>
        <owl:imports rdf:resource="http://www.loa-cnr.it/ontologies/SpatialRelations.owl"/>
        <owl:imports rdf:resource="http://www.loa-cnr.it/ontologies/TemporalRelations.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generic-dependent -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;generic-dependent"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#generically-dependent-on -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;generically-dependent-on">
        <rdfs:range rdf:resource="&DOLCE-Lite;particular"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#has-quale -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;has-quale"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#has-quality -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;has-quality">
        <owl:equivalentProperty rdf:resource="#has-characteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#immediate-relation -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;immediate-relation"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#immediate-relation-i -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;immediate-relation-i"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#inherent-in -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;inherent-in">
        <owl:equivalentProperty rdf:resource="#characteristic-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#part -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;part"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#q-location -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;q-location"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#quale-of -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;quale-of"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#r-location -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;r-location"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#total-temporary-participant -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;total-temporary-participant"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#total-temporary-participant-in -->

    <owl:ObjectProperty rdf:about="&DOLCE-Lite;total-temporary-participant-in"/>
    


    <!-- http://www.loa-cnr.it/ontologies/SpatialRelations.owl#descriptive-place -->

    <owl:ObjectProperty rdf:about="&SpatialRelations;descriptive-place"/>
    


    <!-- http://www.loa-cnr.it/ontologies/SpatialRelations.owl#descriptive-place-of -->

    <owl:ObjectProperty rdf:about="&SpatialRelations;descriptive-place-of"/>
    


    <!-- http://www.loa-cnr.it/ontologies/TemporalRelations.owl#temporal-location -->

    <owl:ObjectProperty rdf:about="&TemporalRelations;temporal-location"/>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#capability-of -->

    <owl:ObjectProperty rdf:about="#capability-of">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;inherent-in"/>
        <rdfs:domain rdf:resource="#capability"/>
        <rdfs:subPropertyOf rdf:resource="#characteristic-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#characteristic-of -->

    <owl:ObjectProperty rdf:about="#characteristic-of">
        <rdfs:comment
            >Inverse of has-characteristic, again introduced to avoid lexical confusion</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;immediate-relation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#code-of -->

    <owl:ObjectProperty rdf:about="#code-of">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;quale-of"/>
        <owl:inverseOf rdf:resource="#has-code"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#data-value-of -->

    <owl:ObjectProperty rdf:about="#data-value-of">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;quale-of"/>
        <owl:inverseOf rdf:resource="#has-data-value"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#described-by-data -->

    <owl:ObjectProperty rdf:about="#described-by-data">
        <rdfs:subPropertyOf rdf:resource="&SpatialRelations;descriptive-place"/>
        <owl:inverseOf rdf:resource="#descriptive-data-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#descriptive-data-of -->

    <owl:ObjectProperty rdf:about="#descriptive-data-of">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >Examples: the temperature of a room, the heart rate of a person). Functional property to allow the retrieval of the physical object based on a given data instance. Inverse described-by-data is not functional.</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >Property that links a data to the physical object it describes (via its characterstics). Subproperty of descriptive-place-of because it represents the (abstract) place where a physical object is described in terms of its characteristics.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&SpatialRelations;descriptive-place-of"/>
        <rdfs:domain rdf:resource="#data"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#duration -->

    <owl:ObjectProperty rdf:about="#duration">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Specialization of #temporal-location describing the concept of duration. It can be applied to states and processes (dolce-lite;stative), but also to dolce-lite;accomplishments. Generically speaking, it is for endurants that are not atomic, but imply a concept of duration over time.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&TemporalRelations;temporal-location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#happens-at -->

    <owl:ObjectProperty rdf:about="#happens-at">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Specialization of #temporal-location for events.</rdfs:comment>
        <rdfs:domain rdf:resource="&DOLCE-Lite;event"/>
        <rdfs:subPropertyOf rdf:resource="&TemporalRelations;temporal-location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-capability -->

    <owl:ObjectProperty rdf:about="#has-capability">
        <rdfs:comment xml:lang="en"
            >*** TBD -- Need to add restrictions on property range ***</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;has-quality"/>
        <rdfs:range rdf:resource="#capability"/>
        <owl:inverseOf rdf:resource="#capability-of"/>
        <rdfs:subPropertyOf rdf:resource="#has-characteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-characteristic -->

    <owl:ObjectProperty rdf:about="#has-characteristic">
        <rdfs:comment
            >Equivalent property to DOLite:has-quality needed to avoid confusion about the term &quot;quality&quot; in SOFIA</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;immediate-relation-i"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-code -->

    <owl:ObjectProperty rdf:about="#has-code">
        <rdfs:comment xml:lang="en"
            >*** TBD -- Need to add restriction on property range: code ***&#160;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;has-quale"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-data-value -->

    <owl:ObjectProperty rdf:about="#has-data-value">
        <rdfs:comment xml:lang="en"
            >*** TBD -- Need to add restriction on property range: data-value ***&#160;</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >This property is not set to functional because the same instance of characteristic measured-by an instance of data might correspond to different values based on different reference-systems.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;has-quale"/>
        <rdfs:range rdf:resource="#data-value"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-identification -->

    <owl:ObjectProperty rdf:about="#has-identification">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >*** TBD -- Need to add restriction to property range ***</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >Range should be identification class</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#has-characteristic"/>
        <rdfs:range rdf:resource="#identification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-name -->

    <owl:ObjectProperty rdf:about="#has-name">
        <rdfs:comment xml:lang="en"
            >*** TBD -- Need to add restriction on property range: name ***</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;has-quale"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#has-state -->

    <owl:ObjectProperty rdf:about="#has-state">
        <rdfs:range rdf:resource="&DOLCE-Lite;state"/>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;total-temporary-participant-in"/>
        <owl:inverseOf rdf:resource="#state-of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#identification-of -->

    <owl:ObjectProperty rdf:about="#identification-of">
        <rdfs:subPropertyOf rdf:resource="#characteristic-of"/>
        <owl:inverseOf rdf:resource="#has-identification"/>
        <rdfs:domain rdf:resource="#identification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#measurand -->

    <owl:ObjectProperty rdf:about="#measurand">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >This property associates each data instance with the particular characteristic instance it measures.
It is functional since each data measures a specific instance of a characteristic. This 1-1 correspondence ensures that it there is at least and only *one* instance of characteristic corresponding to a data instance.
Therefore, the properties has-quale (or better its subproperty has-data-value) and reference-system bind each data instance to a value and a measure unit via the characteristic instance the data measures.
Example: &lt;data123, measurand, temp-x&gt;, &lt;temp-x, inherent-in, roomA&gt;, &lt;temp-x, has-data-value, data-valueZ&gt;,
&lt;data-valueZ, textual-description, &quot;28&quot;&gt;, &lt;data-valueZ, reference-system, Celsius-scale&gt;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generic-dependent"/>
        <rdfs:range rdf:resource="#characteristic"/>
        <rdfs:domain rdf:resource="#data"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#measure-space -->

    <owl:ObjectProperty rdf:about="#measure-space">
        <rdfs:comment xml:lang="en"
            >It associates to each characteristic the measure space(s) where it can be mapped. For example, location can be mapped onto: relative coord. system, absolute coordinates, building plan and so on. This property has in general multiple values: single values must be linked to their respective measure space (== regions) via the property reference-system (functional property).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;q-location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#measured-by -->

    <owl:ObjectProperty rdf:about="#measured-by">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
        <rdfs:domain rdf:resource="#characteristic"/>
        <rdfs:range rdf:resource="#data"/>
        <owl:inverseOf rdf:resource="#measurand"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#name-of -->

    <owl:ObjectProperty rdf:about="#name-of">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;quale-of"/>
        <owl:inverseOf rdf:resource="#has-name"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#performed-by -->

    <owl:ObjectProperty rdf:about="#performed-by">
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property applies to situation when a physical object (e.g., a person) with an agentive nature performs some kind of action/activity. In WP2 we apply it to fault detection. It might need further refinement to represent the concepts of agent and activity.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;perdurant"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;physical-object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#produced-by -->

    <owl:ObjectProperty rdf:about="#produced-by">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
        <rdfs:range rdf:resource="&DOLCE-Lite;physical-object"/>
        <rdfs:domain rdf:resource="#information-object"/>
        <owl:inverseOf rdf:resource="#producer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#producer -->

    <owl:ObjectProperty rdf:about="#producer">
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property states that x has produced or produces data y. The producer can be either a human or a device (e.g., a sensor platform) or any other thing that can be subclassed from physical object.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generic-dependent"/>
        <rdfs:domain rdf:resource="&DOLCE-Lite;physical-object"/>
        <rdfs:range rdf:resource="#information-object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#reference-system -->

    <owl:ObjectProperty rdf:about="#reference-system">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >Note that this property is functional, i.e., each value can only be mapped onto a single reference system</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >This property links the atomic value of a characteristic (e.g., identification, space location, quality-indicator, etc.) to the particular region where this characteristic is measured. Examples: a name has as reference-system a name-system; an IP address has as reference-system the IP-addess-system; a room has as reference-system a building-map-system, ... 
This is a key property to model any characteristic inherent to any object (particular).&#160;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;r-location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#state-of -->

    <owl:ObjectProperty rdf:about="#state-of">
        <rdfs:domain rdf:resource="&DOLCE-Lite;state"/>
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;total-temporary-participant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#used-by -->

    <owl:ObjectProperty rdf:about="#used-by">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generically-dependent-on"/>
        <rdfs:domain rdf:resource="#device"/>
        <rdfs:range rdf:resource="#person"/>
        <owl:inverseOf rdf:resource="#user"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#user -->

    <owl:ObjectProperty rdf:about="#user">
        <rdfs:subPropertyOf rdf:resource="&DOLCE-Lite;generic-dependent"/>
        <rdfs:range rdf:resource="#device"/>
        <rdfs:domain rdf:resource="#person"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#datatype-value -->

    <owl:DatatypeProperty rdf:about="#datatype-value"/>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#measure-unit -->

    <owl:DatatypeProperty rdf:about="#measure-unit">
        <rdfs:subPropertyOf rdf:resource="#datatype-value"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#sofia-timestamp -->

    <owl:DatatypeProperty rdf:about="#sofia-timestamp">
        <rdfs:domain rdf:resource="&DOLCE-Lite;spatio-temporal-particular"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#textual-description -->

    <owl:DatatypeProperty rdf:about="#textual-description">
        <rdfs:comment
            >Names and codes can be described by means of datatypes, e.g., string or integers</rdfs:comment>
        <rdfs:domain rdf:resource="&DOLCE-Lite;quale"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-quality -->

    <owl:Class rdf:about="&DOLCE-Lite;abstract-quality">
        <owl:equivalentClass rdf:resource="#abstract-characteristic"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-region -->

    <owl:Class rdf:about="&DOLCE-Lite;abstract-region"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#event -->

    <owl:Class rdf:about="&DOLCE-Lite;event"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#non-physical-object -->

    <owl:Class rdf:about="&DOLCE-Lite;non-physical-object"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#particular -->

    <owl:Class rdf:about="&DOLCE-Lite;particular"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#perdurant -->

    <owl:Class rdf:about="&DOLCE-Lite;perdurant"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#physical-object -->

    <owl:Class rdf:about="&DOLCE-Lite;physical-object">
        <rdfs:comment xml:lang="en"
            >Any physical object in SOFIA must be identified (e.g., a device or a person).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#physical-quality -->

    <owl:Class rdf:about="&DOLCE-Lite;physical-quality">
        <owl:equivalentClass rdf:resource="#physical-characteristic"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#quale -->

    <owl:Class rdf:about="&DOLCE-Lite;quale"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#quality -->

    <owl:Class rdf:about="&DOLCE-Lite;quality">
        <owl:equivalentClass rdf:resource="#characteristic"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#spatio-temporal-particular -->

    <owl:Class rdf:about="&DOLCE-Lite;spatio-temporal-particular"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#state -->

    <owl:Class rdf:about="&DOLCE-Lite;state"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#temporal-quality -->

    <owl:Class rdf:about="&DOLCE-Lite;temporal-quality">
        <owl:equivalentClass rdf:resource="#temporal-characteristic"/>
    </owl:Class>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#accuracy -->

    <owl:Class rdf:about="#accuracy">
        <rdfs:subClassOf rdf:resource="#quality-indicator"/>
    </owl:Class>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#accuracy-region -->

    <owl:Class rdf:about="#accuracy-region">
        <rdfs:subClassOf rdf:resource="#quality-indicator-region"/>
    </owl:Class>
    


    <!-- http://www.sofia-project-eu/CoreOntology-v1.1.owl#capability -->

    <owl:Class rdf:about="#capability">
        <rdfs:subClassOf rdf:resource="#characteristic"/>
        <rdfs:comment xml:lang="en"
            >This class represents a generic capability. Capabilities might be abstract (e.g., properties of the software), physical (e.g., having a keyboard as input) or temporal (e.g., being available in a certain time-slot).
We do not constrain capabilities to be either abstract or physical or temporal, but leave to the developer the choice of defining a specific capability as the intersection of capability and abstract/physical/temporal characteristic.
Example of definition for a capability:
InputKeyboardCapability = intersection of capability and physical characteristic.</rdfs:comment>
    </owl:Class>
    


