<!-- Object Properties -->
  <owl:FunctionalProperty rdf:about="#accuracyRegion">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:comment xml:lang="en">The region or units of measurement in what accuracy is defined.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#hasQualityMeasurementRegion" />
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="#accuracyValue">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:comment xml:lang="en">The value for measuring accuracy.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#hasQualityDataValue" />
  </owl:FunctionalProperty>
  
  <owl:FunctionalProperty rdf:about="#certaintyRegion">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:comment xml:lang="en">The region or units of measurement in what certainty is defined.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#hasQualityMeasurementRegion" />
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="#certaintyValue">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:comment xml:lang="en">The value for measuring certainty.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#hasQualityDataValue" />
  </owl:FunctionalProperty>
  
  <owl:ObjectProperty rdf:about="#characteristicOf">
    <rdfs:comment xml:lang="en">Inverse of has-characteristic, again introduced to avoid lexical confusion</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="&dolite;immediate-relation"/>
    <owl:equivalentProperty rdf:resource="&dolite;inherent-in"/>
	<owl:inverseOf rdf:resource="#hasCharacteristic"/>	
  </owl:ObjectProperty>

  <!-- AVOID THE USAGE OF CODEOF WHILE IT IS NOT DEFINED -->
  <!--<owl:ObjectProperty rdf:about="#codeOf">-->
