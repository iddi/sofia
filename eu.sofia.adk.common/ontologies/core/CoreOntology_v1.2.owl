  <!--  <rdfs:subPropertyOf rdf:resource="&dolite;q-location"/>-->
  <!--</owl:ObjectProperty>-->

  <owl:ObjectProperty rdf:about="#measuredBy">
    <rdfs:subPropertyOf rdf:resource="&dolite;generically-dependent-on"/>
    <owl:inverseOf rdf:resource="#measurand"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#nameOf">
    <rdfs:subPropertyOf rdf:resource="&dolite;quale-of"/>
    <owl:inverseOf rdf:resource="#hasName"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#observedCharacteristic">
    <rdfs:comment xml:lang="en">The characteristic of a phenomenon subject to observation.</rdfs:comment>
    <rdfs:domain rdf:resource="#Observation"/>
    <rdfs:range rdf:resource="#Characteristic"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="#ownedBy">
    <rdfs:comment xml:lang="en">This property is to identify who is the owner of a smart object of the smart environment.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#specificallyConstantlyDependentOn"/>
    <owl:inverseOf rdf:resource="#owns"/>
    <rdfs:domain rdf:resource="#SmartObject"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#owns">
    <rdfs:comment xml:lang="en">This property is used for specifying which smart object of the environment the user owns, which means that only he or she could use it.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#specificConstantDependent"/>
    <owl:inverseOf rdf:resource="#ownedBy"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#SmartObject"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#processParticipant">
	<rdfs:comment xml:lang="en">This property links the process with the active physical object that is participating in this process.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#participant"/>
	<rdfs:domain rdf:resource="&dolite;process"/>
	<rdfs:range rdf:resource="&dns;agentive-physical-object"/>
  </owl:ObjectProperty>
	
  <owl:FunctionalProperty rdf:about="#producedBy">
	<rdfs:comment xml:lang="en">This property is used to identify which is the physical object that is producing each instance of data included in the ontology.</rdfs:comment>
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="&dolite;generically-dependent-on"/>
    <owl:inverseOf rdf:resource="#producer"/>
    <rdfs:domain rdf:resource="#Data"/>
    <rdfs:range rdf:resource="&dolite;physical-object"/>
  </owl:FunctionalProperty>

  <owl:ObjectProperty rdf:about="#producer">
	<rdfs:comment xml:lang="en">This property is used to identify which is the data that each physical object included in the ontology is producing.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="&dolite;generic-dependent"/>
    <owl:inverseOf rdf:resource="#producedBy"/>
    <rdfs:domain rdf:resource="&dolite;physical-object"/>
    <rdfs:range rdf:resource="#Data"/>
  </owl:ObjectProperty>

  <owl:FunctionalProperty rdf:about="#referenceSystem">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
    <rdfs:comment xml:lang="en">Note that this property is functional, i.e., each value can only be mapped onto a single reference system</rdfs:comment>
    <rdfs:comment xml:lang="en">This property links the atomic value of a characteristic (e.g., identification, space location, quality-indicator, etc.) to the particular region where this characteristic is measured. Examples: a name has as reference-system a name-system; an IP address has as reference-system the IP-addess-system; a room has as reference-system a building-map-system, ... 
This is a key property to model any characteristic inherent to any object (particular).</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="&dolite;r-location"/>
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="#resoultionRegion">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:comment xml:lang="en">The region or units of measurement in what resolution is defined.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#hasQualityMeasurementRegion" />
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="#resolutionValue">
    <rdf:type rdf:resource="&owl;ObjectProperty"/>
	<rdfs:comment xml:lang="en">The value for measuring resolution.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="#hasQualityDataValue" />
  </owl:FunctionalProperty>
  
  <owl:ObjectProperty rdf:about="#specificConstantDependent">
	<rdfs:comment xml:lang="en">This abstract property is to define constant dependencies between two individuals. In other words, dependencies that are permanent along time.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&dolite;immediate-relation"/>
	<owl:inverseOf rdf:resource="#specificallyConstantlyDependentOn"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#specificallyConstantlyDependentOn">
	<rdfs:comment xml:lang="en">This abstract object property is the inverse of the class Specific Constant Dependent. It has the same property definition but has been created to maintain the same structure for the inverse properties located in Immediate Relation.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&dolite;immediate-relation-i"/>
	<owl:inverseOf rdf:resource="#specificConstantDependent"/>
  </owl:ObjectProperty>  
  
  <owl:ObjectProperty rdf:about="#stateParticipant">
	<rdfs:comment xml:lang="en">This property links the state with the participant. That is, it will specify which objects are in each specific state.</rdfs:comment>
	<owl:inverseOf rdf:resource="#isInState"/>
	<rdfs:subPropertyOf rdf:resource="#participant"/>
	<rdfs:domain rdf:resource="&dolite;state"/>
	<rdfs:range rdf:resource="&dns;agentive-physical-object"/>
  </owl:ObjectProperty>
	
  <owl:ObjectProperty rdf:about="#usedBy">
    <rdfs:subPropertyOf rdf:resource="&dolite;generically-dependent-on"/>
    <owl:inverseOf rdf:resource="#uses"/>
    <rdfs:domain rdf:resource="&dolite;physical-object"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#uses">
    <rdfs:comment xml:lang="en">This property specifies which physical objects of the environment the user is using.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="&dolite;generic-dependent"/>
    <owl:inverseOf rdf:resource="#usedBy"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Device"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&dolite;generic-dependent"/>
  <owl:ObjectProperty rdf:about="&dolite;generically-dependent-on"/>
  <owl:ObjectProperty rdf:about="&dolite;has-quale"/>
  <owl:ObjectProperty rdf:about="&dolite;has-quality">
    <owl:equivalentProperty rdf:resource="#hasCharacteristic"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&dolite;immediate-relation"/>
  <owl:ObjectProperty rdf:about="&dolite;immediate-relation-i"/>

  <owl:ObjectProperty rdf:about="&dolite;part"/>
  <owl:ObjectProperty rdf:about="&dolite;q-location"/>
  <owl:ObjectProperty rdf:about="&dolite;quale-of"/>
  <owl:ObjectProperty rdf:about="&dolite;r-location"/>
  <owl:ObjectProperty rdf:about="&dolce-spatial;descriptive-place"/>
  <owl:ObjectProperty rdf:about="&dolce-spatial;descriptive-place-of"/>

</rdf:RDF>
