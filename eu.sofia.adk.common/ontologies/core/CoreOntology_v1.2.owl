  <owl:Class rdf:about="#SmartSpaceApplication">
    <rdfs:comment xml:lang="en">This class contains the description of the smart user roles inside the smart environment. For example, tenant, visitor, rescuer, public authority, ...</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&dns;agentive-social-object"/>
    <owl:disjointWith rdf:resource="#Society"/>
    <owl:disjointWith rdf:resource="#SocialAgent"/>
	<owl:disjointWith rdf:resource="#SmartUserRole"/>
  </owl:Class>
  
  <owl:Class rdf:about="#SmartData">
    <rdfs:subClassOf rdf:resource="#Data"/>
    <rdfs:comment xml:lang="en">This concept will be a key concept in the SOFIA ontology. Most of the real-time data should pend from this concept.</rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="#SmartObject">
    <rdfs:subClassOf rdf:resource="&dns;agentive-physical-object"/>
    <rdfs:comment xml:lang="en">In terms of SOFIA this means a device with capabilities to interact with a semantic information broker (SIB). The software agents that run on these special devices have the capability to change, read or update the information included in the SIB.</rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="#SmartObjectRelatedEvent">
    <rdfs:subClassOf rdf:resource="#SpatialEvent"/>
	<owl:disjointWith rdf:resource="#SmartUserRelatedEvent"/>
    <rdfs:comment xml:lang="en">This class represents events related to motion of object location.</rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="#SmartUser">
    <rdfs:subClassOf rdf:resource="&dns;agentive-physical-object"/>
    <rdfs:comment xml:lang="en">This class represents the user which have the possibility to interactuate with the environment. In this class personal information is not specified (there is another class for that).</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#SmartUserRelatedEvent">
    <rdfs:subClassOf rdf:resource="#SpatialEvent"/>
	<owl:disjointWith rdf:resource="#SmartObjectRelatedEvent"/>
    <rdfs:comment xml:lang="en">This class represents events related to motion of user location.</rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="#SmartUserRole">
    <rdfs:subClassOf rdf:resource="&dns;agentive-social-object"/>
    <owl:disjointWith rdf:resource="#Society"/>
    <owl:disjointWith rdf:resource="#SocialAgent"/>
	<owl:disjointWith rdf:resource="#SmartSpaceApplication" />
    <rdfs:comment xml:lang="en">This class contains the description of the smart user roles inside the smart environment. For example, tenant, visitor, rescuer, public authority, ...</rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="#SocialAgent">
    <rdfs:comment xml:lang="en">This contains the description of an agent which is member of a society.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&dns;agentive-social-object"/>
  </owl:Class>

  <owl:Class rdf:about="#SocialObject">
    <rdfs:subClassOf rdf:resource="&dolite;non-physical-object"/>
    <rdfs:comment xml:lang="en">This class represents endurant characteristics that are important for a set of agents.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Society">
    <rdfs:comment xml:lang="en">This class contains the information which belongs to a society active entity that gather together many different social agents.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="&dns;agentive-social-object"/>
  </owl:Class>
	
  <owl:Class rdf:about="#SpatialEvent">
    <rdfs:subClassOf rdf:resource="&dolite;event"/>
    <rdfs:comment xml:lang="en">This class represents events related with location. For example, a user has moved to another location, a device is in another place, etc.</rdfs:comment>
  </owl:Class>
	
